{
  "name": "scrnhq/laravel-bakery",
  "description": "An on-the-fly GraphQL Schema generator from Eloquent models for Laravel.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "graphql"
  ],
  "authors": [
    {
      "name": "Erik Gaal",
      "email": "e.gaal@scrn.com"
    },
    {
      "name": "Robert van Steen",
      "email": "r.vansteen@scrn.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "laravel/framework": "5.6.*",
    "webonyx/graphql-php": "^0.12.5",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/database": "^3.4",
    "orchestra/testbench": "^3.4",
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Bakery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bakery\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:watch": "vendor/bin/phpunit-watcher watch"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bakery\\BakeryServiceProvider"
      ],
      "aliases": {
        "Bakery": "Bakery\\Support\\Facades\\Bakery"
      }
    }
  }
}
